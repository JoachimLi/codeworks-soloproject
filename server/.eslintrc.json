{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "keyword-spacing": "error",
    "linebreak-style": ["error", "unix"],
    "no-undef": 0,
    "no-unsued-vars": 0,
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "react/display-name": 0,
    "semi": ["error", "always"],
    "space-before-blocks": "error",
    "space-before-function-paren": "error"
  }
}
